{
  "name": "auto-quote",
  "displayName": "AutoQuote",
  "description": "Auto close quotes and add commas based on user input.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.insertSingleQuote",
    "onCommand:extension.insertDoubleQuote"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertSingleQuote",
        "title": "Insert Single Quote"
      },
      {
        "command": "extension.insertDoubleQuote",
        "title": "Insert Double Quote"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+'",
        "command": "extension.insertSingleQuote",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+'",
        "command": "extension.insertDoubleQuote",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "vscode": "^1.1.37"
  },
  "dependencies": {},
  "author": "Vanshaj Raghuvanshi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/auto-quote"
  },
  "bugs": {
    "url": "https://github.com/your-username/auto-quote/issues"
  },
  "keywords": [
    "quotes",
    "auto quote",
    "insert quotes",
    "coding assistant",
    "visual studio code"
  ]
}
